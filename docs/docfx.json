{
    "metadata": [
        {
            "references": [
                {
                    "src": "../moon-dev",
                    "files": [
                        "Assets/Plugins/**.dll"
                    ]
                }
            ],
            "src": [
                {
                    "src": "../moon-dev",
                    "files": [
                        "**.csproj",
                        "**.sln"
                    ]
                }
            ],
            "dest": "api",
            "filter": "filter.yml",
            "disableGitFeatures": false,
            "globalNamespaceId": "Global"
        }
    ],
    "build": {
        "globalMetadata": {
            "_appTitle": "Moon Manual",
            "_appFooter": "Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a> <br> Supported by the <a href=\"https: //dotnetfoundation.org\">.NET Foundation</a>",
            "_copyrightFooter": "Copyright Â© 2023 SEQHORE-SLICE, All rights reserved.",
            "_disableContribution": false,
            "_disableAffix": false,
            "_enableSearch": true
        },
        "postProcessors": [
            "ExtractSearchIndex"
        ],
        "content": [
             "**/*.{md,yml}" 
        ],
        "resource": [
            {
                "files": "resources/**"
            }
        ],
        "template": [
            "default"
        ],
        "xref": [
            "https://nicoco007.github.io/UnityXRefMap/2022.3/xrefmap.yml"
        ],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "dest": "_site"
    }
}